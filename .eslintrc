{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "script",
    "ecmaFeatures": {
      "impliedStrict" : true
    }
  },
  env : {
    "commonjs" : true,
    "browser" : true,
    "node" : true,
    "es6" : true
  },
  "rules": {
    "no-cond-assign" : 1,
    "no-dupe-keys" : 2,
    "no-duplicate-case" : 2,
    "no-empty" : 1,
    "no-unreachable" : 1,
    "eqeqeq" : 1,
    "guard-for-in" : 1,

    "no-negated-in-lhs" : 2,
    "no-sparse-arrays" : 2,
    "wrap-iife" : 2,

    "no-dupe-args" : 2,
    "block-scoped-var" : 2,
    "no-func-assign" : 2,
    "no-label-var" : 2,
    "no-shadow" : 2,
    "no-undefined" : 2,
    "no-unused-vars" : 2,
    "no-use-before-define" : 2,
    "no-undef" : 2,
    "no-redeclare" : 2

  }
}
